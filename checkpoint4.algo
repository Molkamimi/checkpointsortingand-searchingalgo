ALGORITHM  insertion_sort(VAR: ARRAY_OF INTEGER);
VAR
   i,j,k:INTEGER 
BEGIN
    FOR i FROM 1 TO arr.length-1 STEP 1 DO
        key = arr[i]; 
        j = i - 1; 
    

  WHILE (j >= 0 && arr[j] > key) DO
       arr[j + 1] = arr[j]; 
            j = j - 1; 
  END_WHILE
   arr[j + 1] = key
END_FOR
END